,"get_token","$","location","toToggle","get_product_information","get_epidemic_service","dateFormat","fmt","get_slides","get_menu","page","user_group","get","position","json","get_article","size","get_ad","orderby","get_link","get_notice","to_details","id","console","mounted","computed","list_table_application_information","list_table","list_table_article"],"sourceRoot":"src/views","sources":["index.vue"],"sourcesContent":["<template>\n  <div class=\"page_root\" id=\"root_demo\">\n    <div class=\"warp\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col\">\n            <div class=\"card_ad\">\n              <!-- 页头 -->\n              <list_ad\n                :list=\"list_ad\"\n                location=\"店招\"\n                v-if=\"$check_action('/ad/list', 'get')\"\n                style=\"marginbottom: 0.5rem\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"warp\">\n      <div class=\"container swiper_box\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <div class=\"sw_no\">\n              <!-- 轮播图组件 -->\n              <div class=\"swiper_img\">\n                <swiper_img :list=\"list_slide\" />\n              </div>\n                            <div class=\"card_notice\">\n                <div class=\"notice_title\">\n                    公告信息 <router-link to=\"notice/list\">More</router-link>\n                </div>\n                <!-- 公告组件 -->\n                <swiper_notice :list=\"list_notice\" />\n              </div>\n                          </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"warp\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col\">\n            <div class=\"card_ad\">\n              <!-- 顶部 -->\n              <list_ad\n                :list=\"list_ad\"\n                v-if=\"$check_action('/ad/list', 'get')\"\n                location=\"顶部广告\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    \n    <div class=\"warp\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col\">\n            <div class=\"card_ad\">\n              <!-- 中部 -->\n              <list_ad\n                :list=\"list_ad\"\n                v-if=\"$check_action('/ad/list', 'get')\"\n                location=\"中部广告\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n        <div class=\"warp\">\n      <!-- 容器 -->\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <!-- 标题栏组件 -->\n            <div class=\"article_recommend\">\n              <div class=\"title\">\n                <span> 疫情资讯 </span>\n              </div>\n              <div class=\"more_box\">\n                <router-link to=\"/article/list\" class=\"more\">\n                  <span>More</span>\n                </router-link>\n              </div>\n            </div>\n            <!-- 疫情资讯列表组件 -->\n            <list_article v-if=\"show_list_article\" :list=\"list_article\" />\n            <div class=\"overflow-auto\" v-else>\n              <table\n                id=\"list_article\"\n                role=\"table\"\n                aria-busy=\"false\"\n                :aria-colcount=\"fields_article.length\"\n                class=\"table b-table table-striped table-hover\"\n              >\n                <thead>\n                  <tr>\n                    <th v-for=\"(o, i) in fields_article\" :key=\"i\">\n                      {{ o.label }}\n                    </th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr v-for=\"(o, i) in list_table_article\" :key=\"i\">\n                    <td\n                      v-for=\"(oj, n) in fields_goods\"\n                      @click=\"to_details('goods', o)\"\n                    >\n                      <img\n                        v-if=\"oj.type && oj.type == '图片'\"\n                        :src=\"$fullUrl(o[oj.key])\"\n                        alt=\"\"\n                        v-default-img=\"'/img/default.png'\"\n                      />\n                      <span v-else>{{ o[oj.key] }}</span>\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    \n    <div class=\"warp\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col\">\n            <div class=\"card_ad\">\n              <!-- 底部 -->\n              <list_ad\n                :list=\"list_ad\"\n                v-if=\"$check_action('/ad/list', 'get')\"\n                location=\"底部广告\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n\n             <!-- 推荐商品信息模块(开始) -->\n    <div class=\"warp\" v-if=\"$check_action('/product_information/list', 'get')\">\n      <!-- 容器 -->\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <!-- 标题栏组件 -->\n            <div class=\"diy_recommend\">\n              <div class=\"title\">\n                <span> 商品信息推荐 </span>\n              </div>\n              <div class=\"more_box\">\n                <router-link to=\"/product_information/list\" class=\"more\">\n                  <span>More</span>\n                </router-link>\n              </div>\n            </div>\n            <!-- 商品信息推荐列表组件 -->\n            <list_product_information v-if=\"show_list_product_information\" :list=\"list_product_information\" />\n            <div class=\"overflow-auto\" v-else>\n              <table\n                id=\"list_diy\"\n                role=\"table\"\n                aria-busy=\"false\"\n                :aria-colcount=\"fields_product_information.length\"\n                class=\"table b-table table-striped table-hover\"\n              >\n                <thead>\n                  <tr>\n                    <th v-for=\"(o,i) in fields_product_information\" :key=\"i\">\n                      {{ o.label }}\n                    </th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr\n                    v-for=\"(o, i) in list_table_product_information\"\n                    :key=\"i\"\n                    @click=\"to_details('product_information', o, 'product_information_id')\"\n                  >\n                    <td v-for=\"(oj,n) in fields_product_information\">\n                      <img\n                        v-if=\"oj.type && oj.type == '图片'\"\n                        :src=\"$fullUrl(o[oj.key])\"\n                        alt=\"\"\n                        v-default-img=\"'/img/default.png'\"\n                      />\n                      <span v-else-if=\"oj.type && oj.type == 'UID'\">\n                        <span style=\"display: none\">\n                          {{ get_user_by_user_id(o, oj, oj.key) }}\n                        </span>\n                        {{ oj.value }}\n                      </span>\n                      <span v-else>{{ o[oj.key] }}</span>\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- 推荐商品信息模块(结束) -->\n            <!-- 推荐疫情服务模块(开始) -->\n    <div class=\"warp\" v-if=\"$check_action('/epidemic_service/list', 'get')\">\n      <!-- 容器 -->\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col-12\">\n            <!-- 标题栏组件 -->\n            <div class=\"diy_recommend\">\n              <div class=\"title\">\n                <span> 疫情服务推荐 </span>\n              </div>\n              <div class=\"more_box\">\n                <router-link to=\"/epidemic_service/list\" class=\"more\">\n                  <span>More</span>\n                </router-link>\n              </div>\n            </div>\n            <!-- 疫情服务推荐列表组件 -->\n            <list_epidemic_service v-if=\"show_list_epidemic_service\" :list=\"list_epidemic_service\" />\n            <div class=\"overflow-auto\" v-else>\n              <table\n                id=\"list_diy\"\n                role=\"table\"\n                aria-busy=\"false\"\n                :aria-colcount=\"fields_epidemic_service.length\"\n                class=\"table b-table table-striped table-hover\"\n              >\n                <thead>\n                  <tr>\n                    <th v-for=\"(o,i) in fields_epidemic_service\" :key=\"i\">\n                      {{ o.label }}\n                    </th>\n                  </tr>\n                </thead>\n                <tbody>\n                  <tr\n                    v-for=\"(o, i) in list_table_epidemic_service\"\n                    :key=\"i\"\n                    @click=\"to_details('epidemic_service', o, 'epidemic_service_id')\"\n                  >\n                    <td v-for=\"(oj,n) in fields_epidemic_service\">\n                      <img\n                        v-if=\"oj.type && oj.type == '图片'\"\n                        :src=\"$fullUrl(o[oj.key])\"\n                        alt=\"\"\n                        v-default-img=\"'/img/default.png'\"\n                      />\n                      <span v-else-if=\"oj.type && oj.type == 'UID'\">\n                        <span style=\"display: none\">\n                          {{ get_user_by_user_id(o, oj, oj.key) }}\n                        </span>\n                        {{ oj.value }}\n                      </span>\n                      <span v-else>{{ o[oj.key] }}</span>\n                    </td>\n                  </tr>\n                </tbody>\n              </table>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <!-- 推荐疫情服务模块(结束) -->\n         \n    <div class=\"warp\">\n      <div class=\"container\">\n        <div class=\"row\">\n          <div class=\"col\">\n            <div class=\"card_link\">\n              <!-- 底部友情链接 -->\n              <list_link\n                v-if=\"$check_action('/link/list', 'get')\"\n                :list=\"list_link\"\n              />\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n\timport mixin from \"@/mixins/page.js\";\n\t\t\t\timport list_product_information from \"@/components/diy/list_product_information.vue\";\n\t\t\t\timport list_epidemic_service from \"@/components/diy/list_epidemic_service.vue\";\n\t\t\t\timport bar_title from \"@/components/diy/bar_title.vue\";\n\timport list_article from \"@/components/diy/list_article.vue\";\n\timport swiper_img from \"@/components/diy/swiper_img.vue\";\n\timport list_ad from \"@/components/diy/list_ad.vue\";\n\timport list_link from \"@/components/diy/list_link.vue\";\n\timport swiper_notice from \"@/components/diy/swiper_notice.vue\";\n\n\texport default {\n\t\tmixins: [mixin],\n\t\tcomponents: {\n\t\t\t\t\t\tlist_product_information,\n\t\t\t\t\t\tlist_epidemic_service,\n\t\t\t\t\t\tbar_title,\n\t\t\tlist_article,\n\t\t\tswiper_img,\n\t\t\tswiper_notice,\n\t\t\tlist_ad,\n\t\t\tlist_link,\n\t\t},\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\tisSmall: false,\n\t\t\t\tsendValue: \"\",\n\t\t\t\tchatList:[],\n\t\t\t\tshowChat: false,\n\t\t\t\tisAdmin: false,\n\t\t\t\ttoken:\"\",\n        \"list_type_commodity_type\": [],\n\t\t\t\t// 文章模型数组\n\t\t\t\tlist_article: [],\n\t\t\t\tvm_arr_article: [\n\t\t\t\t    \"img\",\n\t\t\t\t    \"title\",\n\t\t\t\t    \"description\",\n\t\t\t\t    \"hits\",\n\t\t\t\t    \"type\",\n\t\t\t\t    \"tag\",\n\t\t\t\t    \"source\",\n\t\t\t\t    \"url\",\n\t\t\t\t    \"create_time\",\n\t\t\t\t],\n\t\t\t\tfields_article: [\n\t\t\t\t    {key: \"img\", label: \"图片\"},\n\t\t\t\t    {key: \"title\", label: \"标题\"},\n\t\t\t\t    {key: \"description\", label: \"描述\"},\n\t\t\t\t    {key: \"hits\", label: \"点击量\"},\n\t\t\t\t    {key: \"type\", label: \"分类\"},\n\t\t\t\t    {key: \"tag\", label: \"标签\"},\n\t\t\t\t    {key: \"source\", label: \"来源\"},\n\t\t\t\t    {key: \"url\", label: \"来源地址\"},\n\t\t\t\t    {key: \"create_time\", label: \"发布时间\"},\n\t\t\t\t],\n\t\t\t\tshow_list_article: true,\n\t\t\t\t\t\t\tshow_list_product_information: true,\n\t\t\t\tlist_product_information: [],\n\t\t\t\tvm_arr_product_information: [\n\t\t\t\t\t\"product_information_id\",\n\t\t\t\t\t\t\t\"store_name\",\n\t\t\t\t\t\t\t\"merchant_information\",\n\t\t\t\t\t\t\t\"trade_name\",\n\t\t\t\t\t\t\t\"commodity_price\",\n\t\t\t\t\t\t\t\"product_picture\",\n\t\t\t\t\t\t\t\"product_introduction\",\n\t\t\t\t\t\t],\n\t\t\t\tfields_product_information: [\n\t\t\t\t\t\t\t\t\t\t{\n\t\t\t\t\t\tkey: \"store_name\",\n\t\t\t\t\t\tlabel: \"店铺名称\",\n\t\t\t\t\t\ttype: \"文本\"\n\t\t\t\t\t},\n\t\t\t\t\t\t\t\t\t\t\t\t\t{\n\t\t\t\t\t\tkey: \"merchant_information\",\n\t\t\t\t\t\tlabel: \"商户信息\",\n\t\t\t\t\t\ttype: \"UID\"\n\t\t\t\t\t},\n\t\t\t\t\t\t\t\t\t\t\t\t\t{\n\t\t\t\t\t\tkey: \"trade_name\",\n\t\t\t\t\t\tlabel: \"商品名称\",\n\t\t\t\t\t\ttype: \"文本\"\n\t\t\t\t\t},\n\t\t\t\t\t\t\t\t\t\t\t\t\t{\n\t\t\t\t\t\tkey: \"commodity_price\",\n\t\t\t\t\t\tlabel: \"商品价格\",\n\t\t\t\t\t\ttype: \"数字\"\n\t\t\t\t\t},\n\t\t\t\t\t\t\t\t\t\t\t\t\t{\n\t\t\t\t\t\tkey: \"product_picture\",\n\t\t\t\t\t\tlabel: \"商品图片\",\n\t\t\t\t\t\ttype: \"图片\"\n\t\t\t\t\t},\n\t\t\t\t\t\t\t\t\t\t\t\t\t{\n\t\t\t\t\t\tkey: \"product_introduction\",\n\t\t\t\t\t\tlabel: \"商品介绍\",\n\t\t\t\t\t\ttype: \"编辑\"\n\t\t\t\t\t},\n\t\t\t\t\t\t\t\t\t],\n\t\t\t\t\t\t\tshow_list_epidemic_service: true,\n\t\t\t\tlist_epidemic_service: [],\n\t\t\t\tvm_arr_epidemic_service: [\n\t\t\t\t\t\"epidemic_service_id\",\n\t\t\t\t\t\t\t\"service_name\",\n\t\t\t\t\t\t\t\"service_type\",\n\t\t\t\t\t\t\t\"release_date\",\n\t\t\t\t\t\t\t\"cover_photo\",\n\t\t\t\t\t\t\t\"service_introduction\",\n\t\t\t\t\t\t],\n\t\t\t\tfields_epidemic_service: [\n\t\t\t\t\t\t\t\t\t\t{\n\t\t\t\t\t\tkey: \"service_name\",\n\t\t\t\t\t\tlabel: \"服务名称\",\n\t\t\t\t\t\ttype: \"文本\"\n\t\t\t\t\t},\n\t\t\t\t\t\t\t\t\t\t\t\t\t{\n\t\t\t\t\t\tkey: \"service_type\",\n\t\t\t\t\t\tlabel: \"服务类型\",\n\t\t\t\t\t\ttype: \"下寻\"\n\t\t\t\t\t},\n\t\t\t\t\t\t\t\t\t\t\t\t\t{\n\t\t\t\t\t\tkey: \"release_date\",\n\t\t\t\t\t\tlabel: \"发布日期\",\n\t\t\t\t\t\ttype: \"日期\"\n\t\t\t\t\t},\n\t\t\t\t\t\t\t\t\t\t\t\t\t{\n\t\t\t\t\t\tkey: \"cover_photo\",\n\t\t\t\t\t\tlabel: \"封面图片\",\n\t\t\t\t\t\ttype: \"图片\"\n\t\t\t\t\t},\n\t\t\t\t\t\t\t\t\t\t\t\t\t{\n\t\t\t\t\t\tkey: \"service_introduction\",\n\t\t\t\t\t\tlabel: \"服务介绍\",\n\t\t\t\t\t\ttype: \"编辑\"\n\t\t\t\t\t},\n\t\t\t\t\t\t\t\t\t],\n\t\t\t\t\t\t\tlist_slide: [],\n\t\t\t\tlist_menu: [],\n\t\t\t\tlist_link: [],\n\t\t\t\tlist_notice: [],\n\t\t\t\tlist_ad: []\n\t\t\t};\n\t\t},\n\t\tcreated(){\n\t\t\tthis.get_token();\n      this.get_list_type_commodity_type();\n\t\t},\n\t\tmethods: {\n\t\t\tget_token(){\n\t\t\t\tlet token = this.$route.query.token\n\t\t\t\tif (token){\n\t\t\t\t\t$.db.set(\"token\",token,120);\n\t\t\t\t\tlocation.href = \"http://localhost:8081/\"\n\t\t\t\t}\n\t\t\t},\n\t\t\t\t\t// toggle\n\t\t\ttoToggle(){\n\t\t\t\tthis.isAdmin = !this.isAdmin;\n\t\t\t},\n\n\t\t\t\t\t\t// 获取商品信息列表\n\t\t\tget_product_information() {\n\t\t\t\tlet url = \"~/api/product_information/get_list?\";\n\t\t\t\t\t\t\t\t// url = \"~/api/product_information/get_list?orderby=hits desc\";\n\t\t\t\t\t\t\t\tthis.$get(url, {\n\t\t\t\t\t\t\t\t\t\"orderby\": \"hits desc\",\n\t\t\t\t\t\t\t\"page\": 1,\n\t\t\t\t\t\"size\": 8\n\t\t\t\t}, (json) => {\n\t\t\t\t\tif (json.result) {\n\t\t\t\t\t\tthis.list_product_information = json.result.list;\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t},\n\t\t\t\t\t\t// 获取疫情服务列表\n\t\t\tget_epidemic_service() {\n\t\t\t\tlet url = \"~/api/epidemic_service/get_list?\";\n\t\t\t\t\t\t\t\t// url = \"~/api/epidemic_service/get_list?orderby=hits desc\";\n\t\t\t\t\t\t\t\tthis.$get(url, {\n\t\t\t\t\t\t\t\t\t\"orderby\": \"hits desc\",\n\t\t\t\t\t\t\t\"page\": 1,\n\t\t\t\t\t\"size\": 8\n\t\t\t\t}, (json) => {\n\t\t\t\t\tif (json.result) {\n\t\t\t\t\t\tthis.list_epidemic_service = json.result.list;\n\t\t\t\t\t}\n\t\t\t\t})\n\t\t\t},\n\t\t\t\n\t\t\t/**\n\t\t\t * 当前年月日时分秒方法\n\t\t\t * @param {Object} fmt\n\t\t\t */\n\t\t\tdateFormat(fmt) {\n\t\t\t\tvar myDate = new Date();\n\t\t\t\tvar o = {\n\t\t\t\t\t\"M+\": myDate.getMonth() + 1, // 月份\n\t\t\t\t\t\"d+\": myDate.getDate(), // 日\n\t\t\t\t\t\"h+\": myDate.getHours(), // 小时\n\t\t\t\t\t\"m+\": myDate.getMinutes(), // 分\n\t\t\t\t\t\"s+\": myDate.getSeconds(), // 秒\n\t\t\t\t\t\"q+\": Math.floor((myDate.getMonth() + 3) / 3), // 季度\n\t\t\t\t\t\"S\": myDate.getMilliseconds() // 毫秒\n\t\t\t\t};\n\t\t\t\tif (/(y+)/.test(fmt))\n\t\t\t\t\tfmt = fmt.replace(RegExp.$1, (myDate.getFullYear() + \"\").substr(4 - RegExp.$1.length));\n\t\t\t\tfor (var k in o)\n\t\t\t\t\tif (new RegExp(\"(\" + k + \")\").test(fmt)) fmt = fmt.replace(RegExp.$1, (RegExp.$1.length == 1) ? (o[k]) : ((\"00\" + o[k]).substr((\"\" + o[k]).length)));\n\t\t\t\t\t\treturn fmt;\n\t\t\t},\n\n\t\t\t// 获取轮播图\n\t\t\tget_slides() {\n\t\t\t    this.$get(\"~/api/slides/get_list?\", {}, (json) => {\n\t\t\t        if (json.result) {\n\t\t\t            this.list_slide = json.result.list;\n\t\t\t        }\n\t\t\t    });\n\t\t\t},\n\t\t\t// 获取导航栏\n\t\t\tget_menu() {\n\t\t\t\tvar user_group = this.user_group;\n\t\t\t    this.$get(\n\t\t\t        \"~/api/auth/get_list?\",\n\t\t\t        {\n\t\t\t            page: \"\",\n\t\t\t\t\t\tuser_group,\n\t\t\t\t\t\tget: 1,\n\t\t\t\t\t\tposition: \"top\"\n\t\t\t        },\n\t\t\t        (json) => {\n\t\t\t            if (json.result) {\n\t\t\t                this.list_menu = json.result.list;\n\t\t\t            }\n\t\t\t        }\n\t\t\t    );\n\t\t\t},\n\t\t\t// 获取文章\n\t\t\tget_article() {\n\t\t\t    this.$get(\n\t\t\t        \"~/api/article/get_list?\",\n\t\t\t        {\n\t\t\t            page: 1,\n\t\t\t            size: 4,\n\t\t\t        },\n\t\t\t        (json) => {\n\t\t\t            if (json.result) {\n\t\t\t                var list_article = json.result.list;\n\t\t\t                this.list_article = list_article;\n\t\t\t            }\n\t\t\t        }\n\t\t\t    );\n\t\t\t},\n\t\t\t// 获取广告\n\t\t\tget_ad() {\n\t\t\t    this.$get(\n\t\t\t        \"~/api/ad/get_list?\",\n\t\t\t        {\n\t\t\t            orderby: '`display` desc'\n\t\t\t        },\n\t\t\t        (json) => {\n\t\t\t            if (json.result) {\n\t\t\t                var list_ad = json.result.list;\n\t\t\t                this.list_ad = list_ad;\n\t\t\t            }\n\t\t\t        }\n\t\t\t    );\n\t\t\t},\n\t\t\t// 获取链接列表\n\t\t\tget_link() {\n\t\t\t    this.$get(\n\t\t\t        \"~/api/link/get_list?\",\n\t\t\t        {\n\t\t\t            page: 1,\n\t\t\t            size: 8,\n\t\t\t        },\n\t\t\t        (json) => {\n\t\t\t            if (json.result) {\n\t\t\t                var list_link = json.result.list;\n\t\t\t                this.list_link = list_link;\n\t\t\t            }\n\t\t\t        }\n\t\t\t    );\n\t\t\t},\n\t\t\t// 获取公告列表\n\t\t\tget_notice() {\n\t\t\t    this.$get(\n\t\t\t        \"~/api/notice/get_list?\",\n\t\t\t        {\n\t\t\t            page: 1,\n\t\t\t            size: 3,\n\t\t\t        },\n\t\t\t        (json) => {\n\t\t\t            if (json.result) {\n\t\t\t                var list_notice = json.result.list;\n\t\t\t                this.list_notice = list_notice;\n\t\t\t            }\n\t\t\t        }\n\t\t\t    );\n\t\t\t},\n\t\t\tto_details(key,o,id) {\n\t\t\t\tif(!id){\n\t\t\t\t\tid = key + \"_id\";\n\t\t\t\t}\n\t\t\t    this.$router.push('/' + key + '/details?' + id + '=' + o[id]);\n\t\t\t},\n\t\t\tasync get_list_type_commodity_type() {\n\t\t\t\tvar json = await this.$get(\"~/api/custom_goods/get_list?groupby=commodity_type\");\n\t\t\t\tif (json.result) {\n\t\t\t\t\tthis.list_type_commodity_type = json.result.list;\n\t\t\t\t\tconsole.log(this.list_type_commodity_type);\n\t\t\t\t\tthis.query.commodity_type = this.list_type_commodity_type[0].commodity_type;\n\t\t\t\t\tthis.get_list();\n\t\t\t\t} else if (json.error) {\n\t\t\t\t\tconsole.log(json.error);\n\t\t\t\t}\n\t\t\t}\n\t\t},\n\t\tmounted() {\n\t\t\t\t\t\tthis.get_product_information();\n\t\t\t\t\t\tthis.get_epidemic_service();\n\t\t\t\t\t\tthis.get_menu();\n\t\t\tthis.get_slides();\n\t\t\tthis.get_article();\n\t\t\tthis.get_link();\n\t\t\tthis.get_notice();\n\t\t\tthis.get_ad();\n\t\t},\n\t\tcomputed: {\n\t\t\t\t\t    list_table_application_information() {\n\t\t        var list = this.list_product_information;\n\t\t        var list_table = [];\n\t\t        for (let i = 0; i < list.length; i++) {\n\t\t            list_table[i] = {};\n\t\t            this.vm_arr_product_information.map((o) => {\n\t\t                // 第二个中括号是对象的属性\n\t\t                list_table[i][o] = list[i][o] || \"\";\n\t\t            });\n\t\t        }\n\t\t        return list_table;\n\t\t    },\n\t\t\t\t\t    list_table_application_information() {\n\t\t        var list = this.list_epidemic_service;\n\t\t        var list_table = [];\n\t\t        for (let i = 0; i < list.length; i++) {\n\t\t            list_table[i] = {};\n\t\t            this.vm_arr_epidemic_service.map((o) => {\n\t\t                // 第二个中括号是对象的属性\n\t\t                list_table[i][o] = list[i][o] || \"\";\n\t\t            });\n\t\t        }\n\t\t        return list_table;\n\t\t    },\n\t\t\t\n\n\t\t    list_table_article() {\n\t\t        var list = this.list_article;\n\t\t        var list_table = [];\n\t\t        for (let i = 0; i < list.length; i++) {\n\t\t            list_table[i] = {};\n\t\t            this.vm_arr_article.map((o) => {\n\t\t                // 第二个中括号是对象的属性\n\t\t                list_table[i][o] = list[i][o] || \"\";\n\t\t            });\n\t\t        }\n\t\t        return list_table;\n\t\t    },\n\t\t}\n\t};\n</script>\n\n<style scoped>\n.page_root {\n  position: relative;\n}\n.support_entry {\n  position: fixed;\n  top: 400px;\n  right